{
  "name": "mosfez-sampler",
  "version": "0.0.0",
  "description": "A microtonal-aware sampler engine for web",
  "main": "./sampler.js",
  "keywords": [
    "web",
    "audio",
    "sampler"
  ],
  "homepage": "https://github.com/dxinteractive/mosfez-sampler",
  "repository": {
    "type": "git",
    "url": "git@github.com:dxinteractive/mosfez-sampler.git"
  },
  "bugs": {
    "url": "https://github.com/dxinteractive/mosfez-sampler/issues"
  },
  "author": "Damien Clarke <dxinteractive@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prep": "yarn && cd dev && yarn",
    "build": "rm -rf dist && yarn rollup -c",
    "test": "yarn jest",
    "lint": "yarn eslint src",
    "pretty": "yarn prettier --write .",
    "prepare": "yarn build && rsync -vt dist/* .",
    "postpublish": "yarn clean",
    "postinstall": "yarn clean",
    "clean": "rm -rf audio-source.* convert.* sampler.* touch-start.*"
  },
  "devDependencies": {
    "@types/audiobuffer-to-wav": "^1.0.2",
    "@types/jest": "^27.5.1",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "esbuild": "^0.14.42",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.29.4",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "2.6.2",
    "rollup": "^2.78.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-string": "^3.0.0",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  },
  "sideEffects": false,
  "files": [
    "audio-source.*",
    "convert.*",
    "sampler.*",
    "touch-start.*"
  ],
  "dependencies": {
    "@types/audiobuffer-to-wav": "^1.0.2",
    "audiobuffer-to-wav": "^1.0.0"
  }
}
